<?xml version="1.0" encoding="UTF-8"?>

<!-- ==== mapper 기본설정 ==== -->
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- ==== 루트 엘리먼트 & 네임스페이스 설정(프로젝트 전체내에서 유일해야 한다.) ==== -->

<mapper namespace="com.spring.app.security.mapper.SecurityMapper">
	
	
	<!-- DB에서 사용자조회 -->
	<select id="findById" parameterType="String" resultType="UsersDTO">
		select *
		from users
		where id = #{id}
	</select>
	
	
</mapper>




